# Local development configuration
# Copy this file to .env.local and modify as needed

# Lambda URLs (required for cloud features)
VITE_LAMBDA_PRESIGN_URL=https://xxxxx.lambda-url.us-east-1.on.aws/
VITE_LAMBDA_SYNC_URL=https://xxxxx.lambda-url.us-east-1.on.aws/
VITE_LAMBDA_INSTANT_URL=https://xxxxx.lambda-url.us-east-1.on.aws/
VITE_LAMBDA_BATCH_URL=https://xxxxx.lambda-url.us-east-1.on.aws/
VITE_LAMBDA_ADMIN_CONFIG_URL=https://xxxxx.lambda-url.us-east-1.on.aws/
VITE_LAMBDA_ADMIN_DELETE_URL=https://xxxxx.lambda-url.us-east-1.on.aws/

# Debug Panel (development only)
# Set to "true" to show debug panel in development mode
# Production builds ignore this setting (always disabled)
VITE_DEBUG_PANEL=true
