# Yorutsuke v2 Environment Variables
# Copy to .env.local and fill in values from CDK outputs
# Or run: npm run env:sync (requires AWS CLI)

# AWS Region
VITE_AWS_REGION=ap-northeast-1

# Cognito
VITE_USER_POOL_ID=
VITE_USER_POOL_CLIENT_ID=

# Lambda URLs (from CDK outputs)
VITE_LAMBDA_PRESIGN_URL=
VITE_LAMBDA_QUOTA_URL=
VITE_LAMBDA_CONFIG_URL=
VITE_LAMBDA_TRANSACTIONS_URL=
VITE_LAMBDA_REPORT_URL=

# Auth API (optional, defaults to Cognito direct)
VITE_AUTH_API_URL=
